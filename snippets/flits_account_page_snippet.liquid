                    <a href="javascript:void(0);" class="flits-wsl-pagination-page {% if i == 1 %}active{% endif %}" data-page="{{ i }}" title="">{{ i }}</a>
                    {% endfor %}
                    <a href="javascript:void(0);" class="flits-wsl-pagination-next"><span>&raquo;</span></a>
                </div>
