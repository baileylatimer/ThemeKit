                            <div class="flits-row flits-text-center">
                                <div class="flits-col-md-12 flits-col-sm-12 flits-product-img-div">
                                    <a target="_blank" class="flits-product-title-anchor">
                                        <div class="flits-product-img" style="background-image: attr('data-image-url');background-size: contain;background-repeat: no-repeat;background-position: center center;">

                                        </div>
                                    </a>
                                </div>
                            </div>
                            <div class="flits-row flits-m-t-10 flits-text-center">
                                <div class="flits-col-md-12 flits-col-sm-12 flits-product-variant-title">

                                </div>
                            </div>
                            <div class="flits-row flits-m-t-10">
                                <div class="flits-col-md-12 flits-col-sm-12">
                                    <select style="width:100%;" class="product-variant-options flits-product-variant-options" name="id">

                                    </select>
                                    <div class="flits-product-quantity">
                                        <input type="text" class="flits-product-quantity-num flits-product-quantity" value="1" min="1" name="quantity">
