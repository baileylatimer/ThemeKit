
{{ 'flits.css' | asset_url | stylesheet_tag }}
{% render 'flits_snippet' with 'style_css' %}
<div class="page-width flits-container">
    <div class="flits-hidden">
        <input type="hidden" name="token" value="{{ shop.metafields.Flits.SHOP_TOKEN }}" id="flits-token">
        <input type="hidden" name="flits-shop-money-format" value="{{ shop.money_format }}" id="flits-shop-money-format">
        <input type="hidden" name="customer-url" value="/apps/{{ shop.metafields.Flits.APP_PROXY }}/api/{{ shop.metafields.Flits.APP_ID }}/{{ shop.metafields.Flits.SHOP_ID }}/{{ customer.id }}" id="flits-customer-url">
        <input type="hidden" name="flits-order-shipment-url" value="/apps/{{ shop.metafields.Flits.APP_PROXY }}/api/{{ shop.metafields.Flits.APP_ID }}/{{ shop.metafields.Flits.SHOP_ID }}" id="flits-order-shipment-url">  
        <input type="hidden" name="flits-order-tracking-paid" value="{{ shop.metafields.Flits.IS_ORDER_TRACKING_PAID }}" id="flits-order-tracking-paid">
        <input type="hidden" name="flits-order-tracking-enable" value="{{ shop.metafields.Flits.IS_ORDER_TRACKING_ENABLE }}" id="flits-order-tracking-enable">
        <input type="hidden" name="flits-order-tracking-domain" value="{{ shop.metafields.Flits.ORDER_TRACKING_DOMAIN }}" id="flits-order-tracking-domain">
        <div class="flits-hidden">
            <li class='flits-card-box' id="flits-customer-address-template">
                <span class="flits-remove-address-btn" data-form-id="{{ customer.default_address.id }}" data-confirm-message="{{ 'flits.address_page.delete_confirm' | t }}">X</span>
                <div class="flits-address">
                    <div class="flits-row">
                        <div class="flits-col-md-12 flits-text-center">
                            <strong>&nbsp;</strong>
                        </div>
                    </div>
                    <div class='flits-address-details'>
                        <div class='flits-row flits-address-name-row'>
                            <div class='flits-col-md-12 flits-content' title="{{ customer.default_address.first_name }} {{ customer.default_address.last_name }}">
                                {{ customer.default_address.first_name }} {{ customer.default_address.last_name }}
                            </div>
                        </div>
                        <div class='flits-row flits-company-row'>
                            <div class='flits-col-md-12 flits-content' title="{{ customer.default_address.company }}">
                                {{ customer.default_address.company }}
                            </div>
                        </div>
                        <div class='flits-row  flits-address1-row'>
                            <div class='flits-col-md-12 flits-content' title="{{ customer.default_address.address1 }}">
                                {{ customer.default_address.address1 }}
                            </div>
                        </div>
                        <div class='flits-row  flits-address2-row'>
                            <div class='flits-col-md-12 flits-content' title="{{ customer.default_address.address2 }}">
                                {{ customer.default_address.address2 }}
                            </div>
                        </div>
                        <div class='flits-row flits-city-row'>
                            <div class='flits-col-md-12 flits-content' title="{{ customer.default_address.zip }} {{ customer.default_address.city }} {{ customer.default_address.province_code }}">
                                {{ customer.default_address.zip }} {{ customer.default_address.city }} {{ customer.default_address.province_code }}
                            </div>
                        </div>
                        <div class='flits-row flits-country-row'>
                            <div class='flits-col-md-12 flits-content' title="{{ customer.default_address.country }}">
                                {{ customer.default_address.country }}
                            </div>
                        </div>
                    </div>
                    <div class='flits-row flits-m-t-20 flits-text-center'>
                        <div class="flits-col-md-6 flits-col-sm-6 flits-col-xs-6 flits-col-md-offset-3 flits-col-sm-offset-3 flits-col-xs-offset-3">
                            <button type="button" class="flits-btn-inline flits-btn-custom flits-btn-default flits-btn flits-full-width flits-btn-small flits-edit-address-btn" data-form-id="{{ customer.default_address.id }}">{{ 'flits.address_page.edit' | t }}</button>
                        </div>
                    </div>
                </div>
            </li>
            <li class="flits-list-group-item" id="flits-credit-log-template">
                <div class="flits-row flits-credit-log-with-dropdown">
                  <div class="flits-col-md-3 flits-credit-col">
                  </div>
                  <div class="flits-col-md-6 flits-credit-comment-col flits-text-muted">
                  </div>
                  <div class="flits-col-lg-3 flits-credit-date-col">
                  </div>
                </div>
                <div class="flits-credit-details" style="display: none;">
                </div>
            </li>
            <div class="flits-row flits-credit-details-template flits-credit-details-row">
              <div class="flits-col-md-3 flits-product-tag-credit">
              </div>
              <div class="flits-col-md-6 flits-product-title flits-text-nowrap">
              </div>
              <div class="flits-col-md-3 flits-product-tag-div">
                <div class="flits-product-tag-svg">
                  <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-tag"><path d="M20.59 13.41l-7.17 7.17a2 2 0 0 1-2.83 0L2 12V2h10l8.59 8.59a2 2 0 0 1 0 2.82z"></path><line x1="7" y1="7" x2="7" y2="7"></line></svg>
                </div>
                <span class="flits-product-tag flits-text-nowrap"></span>
              </div>
            </div>
            <div class="flits-credit-dropdown-svg-div flits-credit-dropdown">
              <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-chevron-down"><polyline points="6 9 12 15 18 9"></polyline></svg>
            </div>
        </div>
    </div>  
    <div>
        <div class="flits-text-left flits-account-page-title mb25">
            <h1>{{ 'flits.general.greeting' | t }}  {{ customer.first_name }}</h1>
        </div>

    </div>
    <div class="flits-page">
        <div class="flits-nav-bar">
            <div class="flits-nav sidebar-content flits-h-full">
                <div class="flits-list-group">
                    <a disabled data-type="flits" data-href="#flits-page-profile" data-toggle='page' class="flits-list-group-item flits-bg-theme">{{ 'flits.navigation.profile' | t }}</a>
                    <a disabled data-type="flits" data-href="#flits-page-orders" data-toggle='page' class="flits-list-group-item active flits-bg-theme">{{ 'flits.navigation.my_orders' | t }}</a>
                    <a disabled data-type="flits" data-href="#flits-page-address" data-toggle='page' class="flits-list-group-item flits-bg-theme">{{ 'flits.navigation.delivery_address' | t }}</a>
                    {% if shop.metafields.Flits.is_store_credit_enable == 0 or shop.metafields.Flits.IS_STORE_CREDIT_PAID == 0 %}

                    {% else %}
                    <a disabled data-type="flits" data-href="#flits-page-credits" data-toggle='page' class="flits-list-group-item flits-bg-theme">{{ 'flits.navigation.store_credit' | t }}</a>
                    {% endif %}

                    {% assign total_pages = shop.metafields.Flits.total_pages %}

                    {% for i in (1..total_pages) %}
                    {% assign title_str = 'title_' | append: i %}
                    {% assign url_str = 'url_' | append:i %}
                    {% assign type_str = 'type_' | append:i %}
                    {% assign title = shop.metafields.Flits[title_str] | capitalize  %}
                    {% assign url = shop.metafields.Flits[url_str] %}
                    {% assign type =  shop.metafields.Flits[type_str] %}
                    {% if type != 'PAGE' %}
                    <a disabled data-type="flits" href="{{ url }}" target="_blank" class="flits-list-group-item flits-bg-theme">{{ title }}</a>
                    {% else %}
                    <a disabled data-type="flits" data-title="{{ title }}" data-toggle='page' data-href="#flits-page-{{ url }}" class="flits-list-group-item flits-bg-theme">{{ title }}</a>          
                    {% endif %}

                    {% endfor %}

                    {% if shop.metafields.Flits.is_recently_view_enable == 0 %}
                    {% else %}
                    <a disabled data-type="flits" data-href="#flits-page-recently-view" data-toggle='page' class="flits-list-group-item flits-bg-theme">{{ 'flits.navigation.recently_viewed_products' | t }}</a>
                    {% endif %}
                    <a disabled data-type="flits" data-href="#flits-page-security" data-toggle='page' class="flits-list-group-item flits-bg-theme">{{ 'flits.navigation.change_password' | t }}</a>
                    <a href="account/logout" class="flits-list-group-item flits-bg-theme">{{ 'flits.navigation.log_out' | t }}</a>
                </div>
            </div>
        </div>
        <div class="flits-page-content">
            <div class="flits-pages flits-h-full">
                <div id="flits-page-profile" data-type="page" class="flits-hidden">
                    <div class="flits-page-heading">
                        <span class="flits-back-icon flits-return-to-menu">
                            {{ 'flits.buttons.back' | t }}
                        </span>
                        <span class="flits-heading">
                            {{ 'flits.heading.my_profile' | t }}
                        </span>
                    </div>
                    <form data-action="profile_save" id="flits-form-profile" method="post">
                        <div class="flits-row">
                            <div class="flits-col-md-6">
                                <label>{{ 'flits.profile_page.first_name' | t }}</label>
                                <label class="flits-hide-profile-edit base-BodyText">{{ customer.first_name }}</label>
                                <input type="text" name="first_name" class="flits-hidden flits-hide-profile-cancel" value="{{ customer.first_name }}">
                            </div>
                        </div>
                        <div class="flits-row flits-m-t-15">
                            <div class="flits-col-md-6">
                                <label>{{ 'flits.profile_page.last_name' | t }}</label>
                                <label class="flits-hide-profile-edit base-BodyText">{{ customer.last_name }}</label>
                                <input type="text" name="last_name" class="flits-hidden flits-hide-profile-cancel" value="{{ customer.last_name }}">
                            </div>
                        </div>
                        <div class="flits-row flits-m-t-15">
                            <div class="flits-col-md-6">
                                <label>{{ 'flits.profile_page.email' | t }}</label>
                                <label class="flits-hide-profile-edit base-BodyText">{{ customer.email }}</label>
                                <input type="email" readonly="" class="flits-hidden flits-hide-profile-cancel" value="{{ customer.email }}" name="email">
                            </div>
                        </div>
                        <div class="flits-row flits-m-t-15">
                            <div class="flits-col-md-6">
                                <label>Phone</label>
                                <label class="flits-hide-profile-edit base-BodyText">{{ customer.phone }}</label>
                                <input type="text" name="phone" class="flits-hidden flits-hide-profile-cancel"  value="{{ customer.phone }}">
                            </div>
                        </div>
                        <div class="flits-row flits-m-t-15">
                            <div class="flits-col-md-6">
                                <label>Birthday</label>
                                <label class="flits-hide-profile-edit base-BodyText">{{ customer.metafields.Flits.birthdate }}</label>
                                <input type="date" name="birthdate" max="2000-12-01" class="flits-profile-birthdate flits-hidden flits-hide-profile-cancel" value="{{ customer.metafields.Flits.birthdate }}">
                            </div>
                        </div>
                        <div class="flits-row flits-m-t-15">
                            <div class="flits-col-md-6">
                                <label>{{ 'flits.profile_page.gender' | t }}</label>
                                <label class="flits-hide-profile-edit flits-profile-gender-label">
                                        {% if customer.metafields.Flits.gender and customer.metafields.Flits.gender != ""%}
                                        {{ 'flits.profile_page.' | append:customer.metafields.Flits.gender | t }}
                                        {% endif %}
                                </label>
                                <div class="flits-m-t-15 flits-hidden flits-hide-profile-cancel">
                                    <select name="gender" class="flits-profile-gender" id="flits-profile-gender">
                                        <option value="male" {% if customer.metafields.Flits.gender == 'male' %}selected{% endif %}>{{ 'flits.profile_page.male' | t }}</option>
                                        <option value="female" {% if customer.metafields.Flits.gender == 'female' %}selected{% endif %}>{{ 'flits.profile_page.female' | t }}</option>
                                        <option value="other" {% if customer.metafields.Flits.gender == 'other' %}selected{% endif %}>{{ 'flits.profile_page.other' | t }}</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="flits-row flits-m-t-10 flits-edit-button-row">
                            <div class="flits-col-md-2">
                                <label>&nbsp;</label>
                                <button type="button" data-type="flits" class='flits-btn-inline flits-btn-block flits-btn flits-btn-default flits-btn-custom flits-profile-edit-button' disabled>{{ 'flits.buttons.edit' | t }}</button>
                            </div>
                        </div>
                        <div class="flits-row flits-m-t-10 flits-save-button-row flits-hidden">
                            <div class="flits-col-md-2">
                                <label>&nbsp;</label>
                                <button type="button" data-type="flits" class='flits-btn-inline flits-btn-block flits-btn-danger flits-btn-custom flits-profile-cancel-button flits-btn' disabled>{{ 'flits.buttons.cancel' | t }}</button>
                            </div>
                            <div class="flits-col-md-2">
                                <label>&nbsp;</label>
                                <button type="submit" data-type="flits" class='flits-btn-inline flits-btn-block flits-btn-primary flits-btn-custom  flits-btn' disabled>{{ 'flits.buttons.save' | t }}</button>
                            </div>
                        </div>
                    </form>
                </div>
                <div id="flits-page-orders" data-type="page">
                    <div class="flits-page-heading">
                        <span class="flits-back-icon flits-return-to-menu">
                            {{ 'flits.buttons.back' | t }}
                        </span>
                        <span class="flits-heading">
                            {{ 'flits.heading.my_orders' | t }}
                        </span>
                    </div>
                    {% assign date_format = 'flits.order_page.order_date_format' | t %}
                    {% if customer.orders.size != 0 %}
                    {% paginate customer.orders by 3 %}
                    {% for order in customer.orders %}
                    
                    {% assign order_date_month_original = order.created_at | date: '%B' %}
                    {% assign order_date_month_downcase = order.created_at | date: '%B' | downcase  %}
                    {% assign order_date_month_text = 'flits.months.' | append:order_date_month_downcase | t %}	
                    {% assign order_date = order.created_at | date:date_format | replace:order_date_month_original,order_date_month_text %}

                    {% assign order_cancelled_month_original = order.cancelled_at | date: '%B' %}
                    {% assign order_cancelled_month_downcase = order.cancelled_at | date: '%B' | downcase  %}
                    {% assign order_cancelled_month_text = 'flits.months.' | append:order_cancelled_month_downcase | t %}	
                    {% assign order_cancelled_date = order.cancelled_at | date:date_format | replace:order_cancelled_month_original,order_cancelled_month_text %}
                    
                    <div class="flits-order-row">
                        <div class="flits-order-status-row">
                            <div class="flits-row">
                                <div class="flits-col-md-6 flits-text-left flits-order-name">{{ 'flits.order_page.order_number' | t }}: <span class='flits-order-details-value'>{{ order.name }}</span></div>
                                <div class="flits-col-md-6 flits-text-right flits-sm-text-left flits-order-total-price" data-value='{{ order.total_price }}'>{{ 'flits.order_page.grand_total' | t }}: <span class='flits-order-details-value'>{{ order.total_price | money }}</span></div>
                            </div>
                        </div>
                        <div class="flits-order-detail-row">
                            <div class="flits-row">
                                <div class="flits-col-md-6 flits-text-left flits-order-date">{{ 'flits.order_page.order_date' | t }}: <span class='flits-order-details-value'>{{ order_date }}</span></div>
                                <div class="flits-col-md-6 flits-text-right flits-sm-text-left flits-order-total-items">{{ 'flits.order_page.total_items' | t }}: <span class='flits-order-details-value'>{{ order.line_items.size }}</span></div>
                            </div>
                            <div class="flits-row">
                                {% if order.cancelled  %}
                                <div class="flits-col-md-6 flits-text-left flits-order-cancel-status" data-value='{{ order.cancelled_at }}'>{{ 'flits.order_page.cancel_at' | t }}: <span class='flits-order-details-value'>{{ order_cancelled_date }}</span></div>                              
                                {% else %}
                                <div class="flits-col-md-6 flits-text-left flits-order-fulfillment-status" data-value='{{ order.fulfillment_status }}'>{{ 'flits.order_page.fulfillment_status' | t }}: <span class='flits-order-details-value'>{{ order.fulfillment_status_label }}</span></div>
                                {% endif %}
                                <div class="flits-col-md-6 flits-text-right flits-sm-text-left flits-order-payment-status" data-value='{{ order.financial_status }}'>{{ 'flits.order_page.payment_status' | t }}: <span class='flits-order-details-value'>{{ order.financial_status_label }}</span></div>
                            </div>

                        </div>
                        <div class="flits-order-items-row">
                            <script>
                              if (typeof window.flits_orders == "undefined"){
                                window.flits_orders = [];
                              }
                              window.flits_orders[{{order.id}}] = [];
                            </script>
                            {% assign order_line_item_price = 0 %}
                            {% for item in order.line_items %}
                            <script>
                              window.flits_orders[{{order.id}}].push({
                                variant_id:'{{ item.variant_id }}',
                                quantity:'{{ item.quantity }}'
                              });
                            </script>
                            <div class="flits-order-item">
                                <div class="flits-row ">
                                    <div class="flits-col-md-2 flits-col-sm-2 flits-col-xs-4 flits-order-item-image">
                                        <img src="{{item.image | img_url: 'small'}}" class="flits-image">
                                        <div class="flits-order-item-quantity">{{ item.quantity }}</div>
                                    </div>
                                    <div class="flits-col-md-10 flits-col-sm-10 flits-col-xs-8">
                                        <div class="flits-row">
                                            <a href="{{ item.url }}" target="_blank">
                                                <div class="flits-col-md-12 flits-order-item-title" title="{{ item.title }}">
                                                    {{ item.title }}
                                                </div>
                                            </a>
                                        </div>
                                        <div class="flits-row flits-m-t-10">
                                            <div class="flits-col-md-12 flits-order-item-price">
                                                {{ item.price | money }}
                                            </div>
                                        </div>
                                        {% if item.fulfillment and item.fulfillment.tracking_number != blank %}
                                        <div class="flits-row flits-m-t-10">
                                            <div class="flits-col-md-12 flits-line-item-track">
                                                {{ 'flits.order_page.tracking_number' | t }} : <a href="{{ item.fulfillment.tracking_url }}" 
                                                                                                  class="flits-order-tracking"
                                                                                                  data-line-item-id="{{ item.id }}"
                                                                                                  data-order-id = "{{ order.id }}"
                                                                                                  data-tracking-number="{{ item.fulfillment.tracking_number }}"
                                                                                                  target="_blank">{{ item.fulfillment.tracking_number }}</a>	                                                
                                            </div>
                                        </div>
                                        {% if shop.metafields.Flits.IS_ORDER_TRACKING_ENABLE and shop.metafields.Flits.IS_ORDER_TRACKING_ENABLE == 1 %}
                                        <div class="flits-m-t-10 flits-hidden flits_append_tracking" data-append-tracking-id="{{ item.id }}">

                                        </div>
                                        <div class="{% if shop.metafields.Flits.IS_ORDER_TRACKING_PAID and shop.metafields.Flits.IS_ORDER_TRACKING_PAID == 1 %}{% else %}flits-hidden{% endif %} flits_append_tracking" data-append-tracking-id="{{ item.id }}">
                                            {% if shop.metafields.Flits.IS_ORDER_TRACKING_PAID and shop.metafields.Flits.IS_ORDER_TRACKING_PAID == 1 %}
                                            {% render 'flits_order_tracking_snippet' with 'order_tracking', is_template:false %}
                                            {% endif %}
                                        </div>
                                        {% endif%}
                                        {% endif %}
                                    </div>
                                </div>
                            </div>
                            {% assign order_line_item_price = order_line_item_price | plus: item.line_price %}
                            {% endfor %}
                            {% if shop.metafields.Flits.IS_ORDER_TRACKING_ENABLE and shop.metafields.Flits.IS_ORDER_TRACKING_ENABLE == 1 %}
                            <div class="{% if shop.metafields.Flits.IS_ORDER_TRACKING_PAID and shop.metafields.Flits.IS_ORDER_TRACKING_PAID == 1 %}{% else %}flits-hidden{% endif %} flits_append_tracking" data-append-tracking-id="{{ order.id }}">
                                {% if shop.metafields.Flits.IS_ORDER_TRACKING_PAID and shop.metafields.Flits.IS_ORDER_TRACKING_PAID == 1 %}
                                {% render 'flits_order_tracking_snippet' with 'order_tracking', is_template:false %}
                                {% endif %}
                            </div>
                            {% endif %}
                        </div>
                        <div class="flits-order-action-row">
                            <div class="flits-row">
                                <div class="flits-col-md-12 flits-text-right">
                                    {% if shop.metafields.Flits.IS_ORDER_TRACKING_ENABLE and shop.metafields.Flits.IS_ORDER_TRACKING_ENABLE == 1 %}
                                    <span class="flits-m-t-10 flits-hidden flits_append_tracking_btn" data-append-tracking-id="{{ order.id }}">
                                    </span>
                                    {% endif %}
                                    <button class="flits-hidden flits-btn-inline flits-btn-primary flits-btn-custom flits-order-details-btn flits-btn" data-show-text="{{ 'flits.order_page.show_order_details' | t }}" data-hide-text="{{ 'flits.order_page.hide_order_details' | t }}">{{ 'flits.order_page.show_order_details' | t }}</button>
                                    
                                    {% if shop.metafields.Flits.IS_REORDER_ENABLE and shop.metafields.Flits.IS_REORDER_ENABLE == 1 %}
                                    <button class="flits-btn-inline flits-btn-default flits-btn-custom flits-order-re-order-btn flits-btn" data-target="{{ order.id }}">{{ 'flits.order_page.re_order' | t }}</button>
                                    {% endif %}

                                    <div class="flits-hidden flits-order-line-items">
                                        
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="flits-order-address-details-row" style="display: none;">
                            <div class="flits-row flits-m-t-15">
                                <div class="flits-col-md-6 flits-col-sm-6 flits-col-xs-12 flits-order-shipping-address">
                                    <h4 class="flits-text-left">{{ 'flits.order_page.shipping_address' | t }}</h4>
                                    {{ order.shipping_address | format_address }}
                                </div>
                                <div class="flits-col-md-6  flits-col-sm-6 flits-col-xs-12 flits-sm-m-t-15  flits-order-billing-address">
                                    <h4 class="flits-text-left">{{ 'flits.order_page.billing_address' | t }}</h4>
                                    {{ order.billing_address | format_address }}
                                </div>
                            </div>
                            <div class="flits-row flits-order-cost-breakup">
                                <div class="flits-col-md-12">
                                    <h4 class="flits-text-left">{{ 'flits.order_page.cost_breakup' | t }}</h4>
                                    <div class="flits-row flits-order-subtotal">
                                        <div class="flits-col-md-6  flits-col-sm-6 flits-col-xs-6 flits-text-left">{{ 'flits.order_page.subtotal' | t }}</div>
                                        <div class="flits-col-md-6 flits-col-sm-6 flits-col-xs-6 flits-text-right">{{ order_line_item_price | money }}</div>
                                    </div>
                                    {% for discount in order.discounts %}
                                    <div class="flits-row flits-order-discount">
                                        <div class="flits-col-md-6 flits-col-sm-6 flits-col-xs-6 flits-text-left">{{ 'flits.order_page.discount_code' | t }}: {{ discount.code }} </div>
